type Mutation {
    createBook(book: CreateBookInput!): Book
    addBookAuthors(bookId: ID!, authorIds: [ID]!): Book

    createAuthor(author: CreateAuthorInput!): Author
    addAuthorBooks(authorId: ID!, bookIds: [ID]!): Author

    createUser(user: CreateUserInput!): User

    createReview(userId: ID!, bookId: ID!, text: String!): Review
}
